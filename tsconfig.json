{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "esModuleInterop": true,
        "declaration": true,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "outDir": "dist/",
        "listFiles": false,
        "noEmitHelpers": false
    },
    "include": ["src/**/*", "effects/effectCreators.ts"],
    "exclude": ["node_modules"],
    "compileOnSave": false
}
